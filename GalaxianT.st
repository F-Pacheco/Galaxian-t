sition - (0@10).			(enemys naves) do:[				:i|				interior := i innerBounds.				((armor intersects: interior) & (i alive)) ifTrue:[						(killFlag )ifTrue:[						"Para evitar matar mas de un 1 alien con un disparo"							i die.							fireFlag := false.						"Para habilitar disparos cuando se mata"							kills:=kills+1.							((i className) = 'PawnAlien') ifTrue:[								kamikCont:= kamikCont +1.				"Para saber cuando atacar kamikaze"								].							armor delete.							killFlag := false.						"Para que la bala no siga matando aliens"							score:= score + (i puntaje).							].					].			].		]		ifFalse:[												"Activa las muertes"				killFlag :=true.				fireFlag :=true.		].		 ]	ifFalse:[														"Inicia la primer bala al iniciar el juego"			self armorForm.		].! !!Logic methodsFor: 'stepping and presenter' stamp: 'Anonymous 11/18/2020 11:48'!moveALiens	|rightReference leftReference|	rightReference := enemys right.	leftReference := enemys left.	((rightReference < (self right))& enemysFlag) ifTrue:[		enemys position: enemys position + (speedX@0)		]		ifFalse:[			enemysFlag :=false.			].	((leftReference  > (self left)) & ( enemysFlag = false))ifTrue:[		enemys position: enemys position - (speedX@0).		]		ifFalse:[			enemysFlag := true.			].! !!Logic methodsFor: 'stepping and presenter' stamp: 'Anonymous 11/18/2020 11:54'!step"Se encarga de manejar las animaciones o movimientos del juego"	self actualizarScore.	self moveALiens.	self moveArmor.	"Aca va el kamikaze"		(kamikCont = 7) ifTrue:[		self selectKamik; atacKamik.		].	(kills=17)ifTrue:[					"Porque hay un kamikaze"		self levelUp.	].! !!Logic methodsFor: 'adding' stamp: 'Anonymous 11/20/2020 14:41'!addGalaxip	galaxip:= Galaxip new.	self addMorph: galaxip.	galaxip bottomLeft: self bottomLeft.! !!Logic methodsFor: 'drawing' stamp: 'Anonymous 11/15/2020 14:06'!armorForm	armor:= Bala new.	armor center: (galaxip center).	self addMorph: armor.