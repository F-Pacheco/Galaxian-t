copiar lista  nombre puntaje  |		contenido:= Array new:2.		lista:= SortedCollection sortBlock:[:a :b| (a at: 2) >= (b at: 2)].		string:= archivo contentsOfEntireFile.		nombre:= String new.		puntaje:= String new.		contenido:= Array new:2.		copiar:= 0.		string do:[:i|									(copiar=0)ifTrue:[				(i ~=$-)ifTrue:[						nombre:= nombre, (i asString).										]ifFalse:[contenido at:1 put: nombre. copiar:=1].]ifFalse:[					(i ~=$#)ifTrue:[puntaje:= puntaje,(i asString)]ifFalse:[contenido at:2 put: (puntaje asInteger). copiar:=0. lista add: contenido. puntaje:= String new. 					nombre:= String new. contenido:= Array new:2.  ].					].				].archivo close.^lista.! !!Logic methodsFor: 'as yet unclassified' stamp: 'Anonymous 11/18/2020 12:08'!levelUp	kills:=0.	speedX:= speedX +10.	speedY:= speedY + 5.	level:= level +1.	(enemys naves)do:[		:i|		i show.		i alive: true.		].	activateKamik := true.	kamikCont := 0.				"Para sabercuando atacar kamikaze"	kamik alive: true.! !!Logic methodsFor: 'as yet unclassified' stamp: 'Anonymous 11/21/2020 22:50'!abrirArchivo		archivo:=  FileStream fileNamed: 'Score.txt'.		(archivo isEmpty)ifTrue:[										"Inicializar arhivo generico"			self crearArchivo.			archivo:=  FileStream fileNamed: 'Score.txt'.		].		^(self ordenarArchivo).! !!Logic methodsFor: 'as yet unclassified' stamp: 'Anonymous 11/21/2020 22:25'!actualizarScore	scoreM delete.	scoreM:=score asMorph.	scoreM position: self topLeft.	scoreM position: scoreM position + (40@0.5).	scoreM color:Color white.	self addMorph: scoreM. ! !!Logic methodsFor: 'accessing' stamp: 'Anonymous 11/16/2020 02:56'!kamikPos	^ kamikPos! !!Logic methodsFor: 'accessing' stamp: 'Anonymous 11/20/2020 14:38'!listaRecords	^ listaRecords! !!Logic methodsFor: 'accessing' stamp: 'Anonymous 11/16/2020 02:24'!speedY	^ speedY! !!Logic methodsFor: 'accessing' stamp: 'Anonymous 11/16/2020 02:24'!speedX: anObject	speedX := anObject! !!Logic methodsFor: 